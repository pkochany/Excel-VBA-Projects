                    wbRimaster.Worksheets(3).Range("B" & write_row).Value = wbForecast.Sheets(1).Range(Cells(current_row, 1), Cells(current_row, 1)).Value
                    wbRimaster.Worksheets(3).Range("C" & write_row).Value = wbForecast.Sheets(1).Range(Cells(5, current_col), Cells(5, current_col)).Value & CLng(CStr(Left(Replace(Replace(CStr(wbForecast.Sheets(1).Range(Cells(6, current_col), Cells(6, current_col))), "W", ""), "-", ""), 2)))
                    wbRimaster.Worksheets(3).Range("D" & write_row).Value = n.Value
                    wbRimaster.Worksheets(3).Range("E" & write_row).Value = 4
                    wbRimaster.Worksheets(3).Range("F" & write_row).Value = "548-1"
                    write_row = write_row + 1
                End If
                current_col = current_col + 1
            Next n
            current_row = current_row + 1
            current_col = 1
        Next i
        'Formatowanie daty z formatu tygodniowego do daty. Na podstawie tygodnia dobiera dzie, zawsze poniedziaek SPRAWDZI!!!'
        Dim forecast_length As Long
        forecast_length = 0
        For Each i In wbRimaster.Worksheets(3).Range("C1:C100000")
            If i.Value = "" Then
                Exit For
            End If
            forecast_length = forecast_length + 1
        Next i
        Dim fc_row As Long
        fc_row = 1
        For Each i In wbRimaster.Worksheets(3).Range("C1:C" & forecast_length)
            wbRimaster.Worksheets(3).Range("G" & fc_row).Value = Left(CStr(i.Value), 4)
            wbRimaster.Worksheets(3).Range("H" & fc_row).Value = Right(CStr(i.Value), Len(CStr(i.Value)) - 4)
            wbRimaster.Worksheets(3).Range("I" & fc_row).Formula = "=DATE(G" & fc_row & ",1,-2)-WEEKDAY(DATE(G" & fc_row & ",1,2))+H" & fc_row & "*7"
            i.Value = wbRimaster.Worksheets(3).Range("I" & fc_row).Value
            wbRimaster.Worksheets(3).Range("C" & fc_row).NumberFormat = "yyyymmdd"
            fc_row = fc_row + 1
        Next i
        'usu kolumny pomocniczne od formatowania'
        wbRimaster.Worksheets(3).Columns("G:I").Delete Shift:=xlShiftToLeft
'======================================================================================================================================================'
        ' Declare file path'
        file_path = Environ("USERPROFILE") & "\Desktop\TXT Forecast 548-1.txt"
        ' Check if file exists, if so delete and save new'
        wbRimaster.Worksheets(3).SaveAs Filename:=file_path, FileFormat:=xlTextWindows, CreateBackup:=False

        ' Clear '
        wbRimaster.Worksheets(3).Columns("A:F").Delete Shift:=xlShiftToLeft
        ' After all files are saved close Forecast excel '
        wbForecast.Close SaveChanges:=False
        succesCounter = succesCounter + 1
    Else
        ' If other file is picked show error msg and close the file '
        MsgBox "Incorrect file, please try again with Arrow Excel."
        wbForecast.Close SaveChanges:=False
    End If
    If succesCounter > 0 Then
        MsgBox "TXT conversion complete."
    End If

End Sub






